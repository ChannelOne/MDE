{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Exporter",
            "type": "chrome",
            "request": "launch",
            "runtimeExecutable": "${workspaceRoot}/scripts/launch.bat",
			"sourceMaps": true
        },
        {
            "name": "Launch",
            "type": "node",
            "program": "${workspaceRoot}/src/index.ts",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy",
                "--es_staging",
                "--harmony-proxies"
            ],
            "env": {
                "Node_ENV": "development"
            },
            "port": 13872,
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/build/**/*.js"
            ]
        },
        {
            "name": "Test",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/build/test/test.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy",
                "--es_staging",
                "--harmony-proxies"
            ],
            "env": {
                "Node_ENV": "development"
            },
            "port": 13872,
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/build/**/*.js"
            ]
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outDir": null,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        },
        {
            "name": "Attach to Process",
            "type": "node",
            "request": "attach",
            "processId": "${command.PickProcess}",
            "port": 5858,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}